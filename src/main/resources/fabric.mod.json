{
  "schemaVersion": 1,
  "id": "matthew_cline_togglewalk",
  "version": "1.2.3",

  "name": "ToggleWalk Mk. II",
  "description": "Makes it so that various controls in Minecraft now toggle instead!",
  "authors": [
    {
      "name": "ExtraCrafTX",
      "contact": {
        "homepage": "https://github.com/ExtraCrafTX"
      }
    },
    {
      "name": "Matthew Cline",
      "contact": {
        "homepage": "https://github.com/matthew-cline"
      }
    }
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/togglewalk-mk-ii",
    "issues": "https://www.curseforge.com/minecraft/mc-mods/togglewalk-mk-ii/issues",
    "sources": "https://github.com/matthew-cline/ToggleWalk_Mk_II"
  },

  "license": "CC BY-NC-SA 4.0",
  "icon": "assets/extracraftx_togglewalk/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "com.extracraftx.minecraft.togglewalk.ToggleWalk"
    ],
    "cotton-client-commands": [
      "com.extracraftx.minecraft.togglewalk.ToggleWalk"
    ]
  },

  "mixins": [
    "extracraftx_togglewalk.mixins.json"
  ],

  "requires": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": ">=1.17.0",
    "java": ">=16"
  },

  "breaks" : {
    "extracraftx_togglewalk": "*"
  },

  "custom": {
    "modmenu:clientsideOnly": true
  }
}
